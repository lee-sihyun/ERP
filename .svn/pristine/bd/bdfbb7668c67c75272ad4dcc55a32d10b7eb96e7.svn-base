	<script>
		$(document).ready(function() {
			App.init();
			EmailCompose.init();
		});
		
		
	

		//var dlehd="${pageContext.servletContext.contextPath }/drafttest";
		
	
		
		$("#submit").click(function(){
			/*보내는사람 아이디*/
			var empNo=$("session.loginUserinfo").val();

			/*내용쓰는거*/
			var writer=$("#writer").val();
			
			
			
			var reinStart=$("#reinStart").val();
		 
			var reinEnd=$("#reinEnd").val(); 
			var reinDate=$("#reinDate").val(); 
		
			var reinContent=$("#reinContent").val();
			
			
			/*결재목록*/	
			var signerOne=$("#signerOne").val();
			var signerTwo=$("#signerTwo").val();
			var signerThree=$("#signerThree").val();
			
			

			
			

			if(writer=="") {
				alert("작성자를 입력해주세요")
				return;
			}
			
		
			if(reinContent=="") {
				alert("복직 사유를 입력해주세요")
				return;
			}
		
		
		
			if(reinStart=="") {
				alert("휴직시작일 입력해주세요")
				return;
			}
			
			if(reinEnd=="") {
				alert("휴직종료일 입력해주세요")
				return;
			}

			if(reinDate=="") {
				alert("복직희망일 입력해주세요")
				return;
			}
		
			
			
			if(signerOne=="") {
				alert("[부장]결재자의 사원번호를 입력해주세요")
				return;
			}
			
			
			
			if(signerTwo=="") {
				alert("[차장]결재자의 사원번호를 입력해주세요")
				return;
			}
		
			if(signerThree=="") {
				alert("[과장]결재자의 사원번호를 입력해주세요")
				return;
			}
		
			
			

			
			$.ajax({
				type:"POST",
				url:"reinstatement",
				data:{"signerOne":signerOne,"signerTwo":signerTwo,"signerThree":signerThree,
				"writer":writer,"reinStart":reinStart, "reinEnd":reinEnd,
				"reinDate":reinDate,"reinContent":reinContent
				
				},
				dateType:"text",
				success: function(text){
					if(text=="success"){
						alert("결재가 완료되었습니다 ");
						location.href="rtdisplay";
						}
				}, 
				error : function(req){
					alert("응답 에러 코드 ="+req.status);
					
				}
			})	
			
			
		});
		
		

		
		
		
	</script>
